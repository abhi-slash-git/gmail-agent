{
	"dependencies": {
		"@ai-sdk/amazon-bedrock": "3.0.68",
		"@ai-sdk/anthropic": "2.0.54",
		"@babel/core": "7.28.5",
		"@electric-sql/pglite": "0.3.14",
		"ai": "5.0.108",
		"babel-plugin-react-compiler": "1.0.0",
		"drizzle-orm": "0.45.0",
		"googleapis": "168.0.0",
		"ink": "6.5.1",
		"ink-select-input": "6.2.0",
		"ink-spinner": "5.0.0",
		"ink-text-input": "6.0.0",
		"open": "11.0.0",
		"react": "19.2.1",
		"tiny-invariant": "1.3.3",
		"turndown": "7.2.2",
		"uuid": "13.0.0",
		"zod": "4.1.13"
	},
	"description": "AI-powered email classification CLI tool",
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/babel__core": "7.20.5",
		"@types/bun": "1.3.4",
		"@types/node": "25.0.0",
		"@types/react": "19.2.7",
		"@types/turndown": "5.0.6",
		"@typescript/native-preview": "7.0.0-dev.20251210.1",
		"drizzle-kit": "0.31.8",
		"husky": "9.1.7"
	},
	"module": "index.tsx",
	"name": "gmail-agent",
	"packageManager": "bun@1.3.4",
	"private": true,
	"repository": {
		"url": "https://github.com/abhi-slash-git/gmail-agent"
	},
	"scripts": {
		"build": "bun scripts/build-js.ts",
		"build:npm": "bun run build && bun run npm:package",
		"check:version": "bun scripts/check-version-bump.ts \"$@\"",
		"db:check": "bun --bun drizzle-kit check",
		"db:generate": "bun --bun drizzle-kit generate && bun scripts/compile-migration.ts",
		"dev": "bun --hot run index.tsx",
		"lint": "biome check --write",
		"npm:package": "bun scripts/generate-npm-packages.ts",
		"postinstall": "bun scripts/copy-pglite-assets.ts",
		"prepare": "husky",
		"start": "bun run index.tsx",
		"test": "bun test src",
		"test:all": "bun run test && bun run test:isolated:pglite && bun run test:isolated:connection && bun run test:isolated:classifier",
		"test:all:coverage": "bun test --coverage src && bun test --coverage tests-isolated/pglite-wrapper.test.ts && bun test --coverage tests-isolated/connection.test.ts && bun test --coverage tests-isolated/parallel-classifier.test.ts",
		"test:coverage": "bun test --coverage src",
		"test:isolated:classifier": "bun test tests-isolated/parallel-classifier.test.ts",
		"test:isolated:connection": "bun test tests-isolated/connection.test.ts",
		"test:isolated:pglite": "bun test tests-isolated/pglite-wrapper.test.ts",
		"tsc": "tsgo --noEmit 2>&1 | head -50"
	},
	"type": "module",
	"version": "1.0.1"
}
